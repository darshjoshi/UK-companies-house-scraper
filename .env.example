# Required Environment Variables for Netlify Deployment

# Anthropic API Key (required for AI analysis)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: BrowserBase configuration for remote browser (recommended for production)
BROWSERBASE_API_KEY=your_browserbase_api_key_here
BROWSERBASE_PROJECT_ID=your_browserbase_project_id_here

# Optional: Custom configuration
MAX_PAGES_LIMIT=50
MAX_PEOPLE_PAGES_LIMIT=15
REQUEST_TIMEOUT=300000
